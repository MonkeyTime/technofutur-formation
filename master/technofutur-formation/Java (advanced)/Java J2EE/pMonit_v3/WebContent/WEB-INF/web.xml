<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>pMonit_v3</display-name>
  <welcome-file-list>
    <welcome-file>main/index</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>serial</param-name>
    <param-value>C:\temp\</param-value>
  </context-param>
  <servlet>
    <servlet-name>HomeServlet</servlet-name>
    <servlet-class>be.technotic.monitoring.controller.servlet.HomeServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HomeServlet</servlet-name>
    <url-pattern>/main/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Profiler</servlet-name>
    <servlet-class>be.technotic.monitoring.controller.servlet.Profiler</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Profiler</servlet-name>
    <url-pattern>/stats</url-pattern>
    <url-pattern>/stat</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>be.technotic.monitoring.controller.listener.AppListener</listener-class>
  </listener>
  <filter>
    <display-name>AccessFilter</display-name>
    <filter-name>AccessFilter</filter-name>
    <filter-class>be.technotic.monitoring.controller.filter.AccessFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>AccessFilter</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
  <servlet>
    <description></description>
    <display-name>AuthServlet</display-name>
    <servlet-name>AuthServlet</servlet-name>
    <servlet-class>be.technotic.monitoring.controller.servlet.AuthServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AuthServlet</servlet-name>
    <url-pattern>/AuthServlet</url-pattern>
  </servlet-mapping>
</web-app>